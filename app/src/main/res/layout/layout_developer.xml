<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout_bottom_sheet_developer"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/header_round"
    app:behavior_hideable="true"
    app:behavior_peekHeight="0dp"
    app:layout_behavior="@string/bottom_sheet_behavior">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/marvel_regular"
        android:text="DESAFIO ANDROID MARVEL\nEVANDRO COSTA"
        android:textAlignment="center"
        android:textColor="@color/colorWhite"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="122dp"
        android:layout_height="127dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/header_round"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/layout_developer_imageView_email"
        app:srcCompat="@drawable/ic_photo" />

    <ImageView
        android:id="@+id/layout_developer_imageView_linkedin"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginTop="16dp"
        android:background="?android:attr/selectableItemBackground"
        android:clickable="true"
        android:focusable="true"
        android:src="@drawable/ic_linkedin"
        app:layout_constraintEnd_toStartOf="@+id/layout_developer_imageView_email"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <ImageView
        android:id="@+id/layout_developer_imageView_email"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="?android:attr/selectableItemBackground"
        android:clickable="true"
        android:focusable="true"
        android:src="@drawable/ic_yahoo"
        app:layout_constraintBottom_toBottomOf="@+id/layout_developer_imageView_linkedin"
        app:layout_constraintEnd_toStartOf="@+id/layout_developer_imageView_whatsapp"
        app:layout_constraintStart_toEndOf="@+id/layout_developer_imageView_linkedin"
        app:layout_constraintTop_toTopOf="@+id/layout_developer_imageView_linkedin" />

    <ImageView
        android:id="@+id/layout_developer_imageView_whatsapp"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginEnd="8dp"
        android:background="?android:attr/selectableItemBackground"
        android:clickable="true"
        android:focusable="true"
        android:src="@drawable/ic_whatsapp"
        app:layout_constraintBottom_toBottomOf="@+id/layout_developer_imageView_email"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/layout_developer_imageView_email"
        app:layout_constraintTop_toTopOf="@+id/layout_developer_imageView_email" />

</androidx.constraintlayout.widget.ConstraintLayout>